<UserControl x:Class="ListBoxDemo.Controls.ModelTypeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
             xmlns:local="clr-namespace:ListBoxDemo.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="420">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch">
            <svgc:SvgViewbox x:Name="img" Width="40" Height="48" Margin="30,0,0,0"/>
            <TextBlock x:Name="modelTypeTxt" Margin="30,0,0,0" VerticalAlignment="Center"/>
        </StackPanel>
    </Grid>
</UserControl>
